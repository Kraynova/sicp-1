(define (square x) * x x)
(define (sum-of-square a b)
  (+ (square a) (square b)))

(define (2biggest a b c)
  (cond ((and (< a b) (< a c)) (sum-of-square b c))
        ((and (< b a) (< b c)) (sum-of-square a c))
        ((and (< c a) (< c b)) (sum-of-square a b))
        (else 99)))

